Algoritmo FOR_contar_FOR
	Definir contador Como Entero;
	
	Para contador = 0 Hasta 1000 Con Paso 10 Hacer
		Escribir "contador: ", contador;
	Fin Para
	
	contador = 0;
	Mientras contador <= 100 Hacer
		Escribir "contador del while: ", contador;
		contador = contador + 10;
	Fin Mientras
	
FinAlgoritmo
